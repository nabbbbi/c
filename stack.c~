#include <stdio.h>
#include <stdlib.h>

struct item {
  int data;
  struct item *next;
};

struct item *int_array_to_list(int *arr, int len)
{
  struct item *first=NULL, *tmp;
  int i;
  for (i=0; i < len; i++)
    {
      tmp = malloc(sizeof(struct item));
      tmp->data=arr[i];
      tmp->next=first;
      first=tmp;
    }
  return first;
}

int main()
{
  struct item *f; 
  int m[] = {3,5,5,2,5,1};
  f = int_array_to_list(m,sizeof(m));
  int i;
  while(f->next)
    {
      printf("%d\n",f->data);
      f=f->next;
    }
  for (i=0; i< sizeof(m)/4; i++)
    {}
  printf("the first intex is: %d\n size of array: %ld\n size of struct: %ld\n",f->data, sizeof(m), sizeof(*f));
  return 0;
}
